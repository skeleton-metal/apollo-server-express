    "mongo-cursor-pagination": "^7.2.0",
    "saslprep": "^1.0.3",
    "mongoose-unique-validator": "^2.0.3",
    "nodemailer": "^6.3.1",
    "nodemailer-pug-engine": "^1.0.1",
    "winston": "^3.2.1",
    "node-device-detector": "^1.2.1",
    "geoip-lite": "^1.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.4",
    "@babel/node": "^7.8.7",
    "@babel/core": "^7.8.7",
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "^7.8.7"
